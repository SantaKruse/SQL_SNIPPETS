CREATE	FUNCTION	[dbo].[UF_EGFR_ETH_ADJ_2] 
(
			@IN1	AS	VARCHAR(40)
			, @IN2	AS	FLOAT
)
RETURNS		FLOAT
AS
BEGIN
			DECLARE		@OUT	AS	FLOAT
			SET @OUT = CASE WHEN @IN1 IN (SELECT <INSERT PATIENT_ID> FROM <INSERT TABLE ID> WHERE ETHNICITY = <ENTER BLACK ETHNICITY VALUE>) THEN @IN2 * 1.210 ELSE @IN2 END		
			RETURN	@OUT
END

GO
